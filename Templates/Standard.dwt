<!-- LOGIN MODAL -->
<div id="loginModal" class="reveal-modal medium">
	<div class="modal-upper">
    	<div class="row half-spacer hide-for-small"></div>
        <div class="row quarter-spacer show-for-small"></div>
    	<div class="row modal-header">
        	<div class="four columns mobile-one">
            	<img src="/images/icons/lock.png" class="right">
            </div>
            <div class="eight columns mobile-three">
            	<h2>Login to Bellinity</h2>
            </div>
        </div>
    </div>
    <div class="modal-upper-shadow"></div>
    <div class="row triple-spacer"></div>
    <form class="custom">
    <div class="row">
    	<div class="nine columns centered">
        	<div class="row">
            	<div class="four columns">
                	<label class="right inline">Username<label>
                </div>
                <div class="seven columns">
                	<input type="text" class="twelve" />
                </div>
                <div class="one columns"></div>
            </div>
        </div>
    </div>
    <div class="row">
    	<div class="nine columns centered">
        	<div class="row">
            	<div class="four columns">
                	<label class="right inline">Password<label>
                </div>
                <div class="seven columns">
                	<input type="password" class="twelve" />
                </div>
                <div class="one columns"></div>
            </div>
        </div>
    </div>
    <div class="row quarter-spacer hide-for-small"></div>
    <div class="row">
    	<div class="six columns centered">
        	<a href="/dashboard" class="button radius medium expand">Login</a>
        </div>
    </div>
    <div class="row half-spacer"></div>
    </form>
    <div class="modal-lower">
    	<div class="modal-lower-left"><img src="/images/icon_modal_info.png"><p>Not a member, <a href="#" data-reveal-id="createaccountModal">sign up here</a></p></div>
    </div>
</div>
<div id="createaccountModal" class="reveal-modal medium">
	<div class="modal-upper">
    	<div class="row half-spacer hide-for-small"></div>
        <div class="row quarter-spacer show-for-small"></div>
    	<div class="row modal-header">
        	<div class="four columns mobile-one">
            	<img src="/images/icons/lock.png" class="right">
            </div>
            <div class="eight columns mobile-three">
            	<h2>Create Account</h2>
            </div>
        </div>
    </div>
    <div class="modal-upper-shadow"></div>
    <div class="row triple-spacer"></div>
    <div class="row">
    	<div class="nine columns centered">
        <!-- INNER MODAL CONTENT GOES HERE -->
        <form>
        <div class="row">
        	<div class="six columns mobile-two">
            	<a href=""><div class="create-account-choose-type">
                	<img src="/images/create_account_user_icon.png">
                    <h6 class="text-center uppercase remove-margin">Member</h6>
                </div></a>
            </div>
            <div class="six columns mobile-two">
            	<a href=""><div class="create-account-choose-type">
                	<img src="/images/create_account_user_icon.png">
                    <h6 class="text-center uppercase remove-margin">Professional</h6>
                </div></a>
            </div>
        </div>
        <div class="row">
        	<div class="ten columns centered">
        		<div class="center">
                	<a href=""><img src="/images/create_account_facebook.png" class="create-account-social	"></a>
                	<a href=""><img src="/images/create_account_google.png"  class="create-account-social	"></a>
                </div>
            </div>
        </div>
        <div class="row">
        	<div class="ten columns centered">
            	<div class="create-account-spacer-or hide-for-small">or</div>
                <div class="create-account-spacer"></div>
            </div>
        </div>
        <div class="row">
        	<div class="ten columns centered">
            	<input type="text" placeholder="First Name">
                <input type="text" placeholder="Last Name">
                <input type="text" placeholder="Email Address">
                <input type="password" placeholder="Password">
                <input type="password" placeholder="Password Confirmation">
                <a href="/edit-profile" class="button medium expand radius">Create Account</a>
            </div>
        </div>
        </form>
        <!-- END INNER MODAL CONTENT -->
        </div>
    </div>
    <div class="row half-spacer"></div>
    <div class="modal-lower">
    	
    </div>
</div>
